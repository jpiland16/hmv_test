{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./src", "server.js"]
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/component",
        "node_modules/better-docs/category"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "jsdoc/",
        "recurse": true,
        "template": "./node_modules/better-docs",
        "tutorials": "./http-tutorials",
        "readme": "readme.md"    
    },
    "templates": {
        "better-docs": {
            "name": "hmv_test - documentation"
        }
    }
  }