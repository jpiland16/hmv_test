{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["src", "server.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {            
        "destination": "./jsdoc",        
        "recurse": true,  
        "tutorials": "./http-tutorials"               
    }
}